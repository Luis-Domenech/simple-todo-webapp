    "next": "^13.2.4",
    "next-transpile-modules": "^10.0.0",
    "node-fetch": "^2.6.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-input-mask": "^2.0.4",
    "react-number-format": "^5.1.4",
    "tozod": "^3.0.0",
    "uuid": "^9.0.0",
    "web-vitals": "^3.3.0",
    "zod": "^3.21.4",
    "zustand": "^4.3.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
