{
  "name": "simple-todo-webapp",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.14.0 <17.0.0"
  },
  "scripts": {
    "start": "cross-env BROWSER=none next start",
    "gen": "lfd -c lfd-config.yml -e .env.local",
    "build": "cross-env NODE_ENV=production lfd -c lfd-config.yml && next build",
    "q-build": "cross-env NODE_ENV=production next build",
    "export": "next export",
    "dev": "next dev",
    "lint": "next lint --quiet",
    "test": "next test",
    "tests": "ts-node ./src/tests/tests.tsx",
    "gen-env": "gen-env-types .env.local -o src/types/env.d.ts -e .",
    "lint-staged": "lint-staged",
    "ngrok-http": "ngrok http 3000"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.5.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
