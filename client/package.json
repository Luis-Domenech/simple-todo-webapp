    "@next/font": "^13.2.4",
    "@tanstack/react-query": "^4.12.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^18.7.5",
    "@types/react": "^18.0.32",
    "@types/react-dom": "^18.0.11",
    "aos": "^2.3.4",
    "browser-image-compression": "^2.0.0",
